{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt -std=c++20 && $dir$fileNameWithoutExt < in.txt && rm $dir$fileNameWithoutExt"
    },
    "files.associations": {
        "*.css": "tailwindcss",
        "vector": "cpp",
        "bitset": "cpp",
        "queue": "cpp"
    },
    "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, BreakBeforeBraces: Attach }",
    "[cpp]": {
        "editor.tabSize": 2
    }
}